{
	"$schema": "https://biomejs.dev/schemas/1.9.4/schema.json",
	"linter": {
		"enabled": true,
		"rules": {
			"complexity": {
				"noForEach": "off"
			},
			"style": {
				"useImportType": "off",
				"useNodejsImportProtocol": "off"
			},
			"suspicious": {
				"noExplicitAny": "off"
			}
		}
	}
}
